{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", 4],
        "array-bracket-spacing" : 0,
        "space-before-function-paren" : ["error", "never"],
        "space-in-parens": 0
    }
}
